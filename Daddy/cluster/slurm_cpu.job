#!/bin/bash
# SLURM job for slim hierarchical DQN (CPU)

#SBATCH -p linuxlab
#SBATCH -t 04:00:00
#SBATCH -c 8
#SBATCH -J slim-dqn-cpu
#SBATCH -o slim_cpu_%j.out
#SBATCH -e slim_cpu_%j.err

export CONDA_ENVS_DIRS="$HOME/conda_envs/"
export CONDA_PKGS_DIRS="$HOME/conda_pkgs/"
source ~/.bashrc
conda activate pokemon_slim_cpu

python -m Daddy.train_slim --total-steps 100000 --num-envs 4 --headless --device cpu
